{
  "name": "homepage",
  "version": "1.0.0",
  "description": "Personal homepage for Christo Stephenson",
  "main": "index.html",
  "devDependencies": {
    "semantic-ui-css": "^2.4.1",
    "node-sass": "^7.0.0",
    "tsparticles": "^1.26.2",
    "typescript": "^4.2.4",
    "parcel-bundler": "^1.9.7",
    "cross-env": "^5.2.0",
    "copy": "^0.3.2",
    "gh-pages": "^3.1.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel serve src/index.html",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url .",
